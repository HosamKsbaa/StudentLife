{
    "openapi": "3.1.0",
    "info": {
        "title": "FastAPI",
        "version": "0.1.0"
    },
    "paths": {
        "/student/{student_id}/history": {
            "get": {
                "summary": "Get Student",
                "operationId": "get_student_student__student_id__history_get",
                "parameters": [
                    {
                        "name": "student_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "title": "Student Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HDStudent"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/student/{student_id}/progress": {
            "get": {
                "summary": "Get Student Progress",
                "operationId": "get_student_progress_student__student_id__progress_get",
                "parameters": [
                    {
                        "name": "student_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Student Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/students/email/{email}": {
            "get": {
                "summary": "Get Student Profile By Email",
                "operationId": "get_student_profile_by_email_students_email__email__get",
                "parameters": [
                    {
                        "name": "email",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Email"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/XProfile"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/student/{student_id}/progress2": {
            "get": {
                "summary": "Get Student Progress2",
                "operationId": "get_student_progress2_student__student_id__progress2_get",
                "parameters": [
                    {
                        "name": "student_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Student Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RAResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/student/{student_name}": {
            "get": {
                "summary": "Read Student",
                "operationId": "read_student_student__student_name__get",
                "parameters": [
                    {
                        "name": "student_name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Student Name"
                        }
                    },
                    {
                        "name": "benchmarks",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            },
                            "title": "Benchmarks"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/StudentResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Category": {
                "properties": {
                    "code": {
                        "type": "string",
                        "title": "Code"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "amount": {
                        "type": "number",
                        "title": "Amount"
                    },
                    "sub_categories": {
                        "items": {
                            "$ref": "#/components/schemas/SubCategory"
                        },
                        "type": "array",
                        "title": "Sub Categories"
                    }
                },
                "type": "object",
                "required": [
                    "code",
                    "name",
                    "amount",
                    "sub_categories"
                ],
                "title": "Category"
            },
            "Certificate": {
                "properties": {
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "issuer": {
                        "type": "string",
                        "title": "Issuer"
                    },
                    "date": {
                        "type": "string",
                        "title": "Date"
                    },
                    "credentialId": {
                        "type": "string",
                        "title": "Credentialid"
                    }
                },
                "type": "object",
                "required": [
                    "title",
                    "issuer",
                    "date",
                    "credentialId"
                ],
                "title": "Certificate"
            },
            "CompetenciesStudent": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "categories": {
                        "items": {
                            "$ref": "#/components/schemas/Category"
                        },
                        "type": "array",
                        "title": "Categories"
                    }
                },
                "type": "object",
                "required": [
                    "name",
                    "categories"
                ],
                "title": "CompetenciesStudent"
            },
            "CourseInfo": {
                "properties": {
                    "code": {
                        "type": "string",
                        "title": "Code"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "originalCode": {
                        "type": "string",
                        "title": "Originalcode"
                    },
                    "credits": {
                        "type": "integer",
                        "title": "Credits"
                    },
                    "expectedGrade": {
                        "type": "number",
                        "title": "Expectedgrade"
                    },
                    "category": {
                        "type": "string",
                        "title": "Category"
                    },
                    "is_available_this_term": {
                        "type": "boolean",
                        "title": "Is Available This Term"
                    },
                    "typical_year": {
                        "type": "integer",
                        "title": "Typical Year"
                    },
                    "term": {
                        "type": "integer",
                        "title": "Term"
                    },
                    "category_progress": {
                        "type": "string",
                        "title": "Category Progress"
                    },
                    "dependent_courses_count": {
                        "type": "integer",
                        "title": "Dependent Courses Count"
                    },
                    "dependency_tree": {
                        "$ref": "#/components/schemas/DependencyTree"
                    }
                },
                "type": "object",
                "required": [
                    "code",
                    "name",
                    "originalCode",
                    "credits",
                    "expectedGrade",
                    "category",
                    "is_available_this_term",
                    "typical_year",
                    "term",
                    "category_progress",
                    "dependent_courses_count",
                    "dependency_tree"
                ],
                "title": "CourseInfo"
            },
            "DependencyTree": {
                "properties": {
                    "code": {
                        "type": "string",
                        "title": "Code"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "typical_year": {
                        "type": "integer",
                        "title": "Typical Year"
                    },
                    "term": {
                        "type": "integer",
                        "title": "Term"
                    },
                    "category": {
                        "type": "string",
                        "title": "Category"
                    },
                    "credits": {
                        "type": "integer",
                        "title": "Credits"
                    },
                    "dependencies": {
                        "items": {
                            "$ref": "#/components/schemas/DependencyTree"
                        },
                        "type": "array",
                        "title": "Dependencies",
                        "default": []
                    }
                },
                "type": "object",
                "required": [
                    "code",
                    "name",
                    "typical_year",
                    "term",
                    "category",
                    "credits"
                ],
                "title": "DependencyTree"
            },
            "Experience": {
                "properties": {
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "company": {
                        "type": "string",
                        "title": "Company"
                    },
                    "location": {
                        "type": "string",
                        "title": "Location"
                    },
                    "period": {
                        "type": "string",
                        "title": "Period"
                    }
                },
                "type": "object",
                "required": [
                    "title",
                    "company",
                    "location",
                    "period"
                ],
                "title": "Experience"
            },
            "HDCourse": {
                "properties": {
                    "course_id": {
                        "type": "string",
                        "title": "Course Id"
                    },
                    "course_name": {
                        "type": "string",
                        "title": "Course Name"
                    },
                    "course_section": {
                        "type": "integer",
                        "title": "Course Section"
                    },
                    "credit": {
                        "type": "number",
                        "title": "Credit"
                    },
                    "final_grade": {
                        "type": "string",
                        "title": "Final Grade",
                        "default": "N/A"
                    },
                    "gpa": {
                        "type": "number",
                        "title": "Gpa",
                        "default": 0.0
                    }
                },
                "type": "object",
                "required": [
                    "course_id",
                    "course_name",
                    "course_section",
                    "credit"
                ],
                "title": "HDCourse"
            },
            "HDStudent": {
                "properties": {
                    "people_id": {
                        "type": "integer",
                        "title": "People Id"
                    },
                    "program": {
                        "type": "string",
                        "title": "Program"
                    },
                    "degree": {
                        "type": "string",
                        "title": "Degree"
                    },
                    "curriculum": {
                        "type": "string",
                        "title": "Curriculum"
                    },
                    "college": {
                        "type": "string",
                        "title": "College"
                    },
                    "department": {
                        "type": "string",
                        "title": "Department"
                    },
                    "years": {
                        "items": {
                            "$ref": "#/components/schemas/HDYear"
                        },
                        "type": "array",
                        "title": "Years",
                        "default": []
                    },
                    "total_gpa": {
                        "type": "number",
                        "title": "Total Gpa",
                        "default": 0.0
                    },
                    "total_credits": {
                        "type": "integer",
                        "title": "Total Credits",
                        "default": 0
                    },
                    "level": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/StudentLevel"
                            }
                        ],
                        "default": "Freshman"
                    }
                },
                "type": "object",
                "required": [
                    "people_id",
                    "program",
                    "degree",
                    "curriculum",
                    "college",
                    "department"
                ],
                "title": "HDStudent"
            },
            "HDTerm": {
                "properties": {
                    "term_name": {
                        "type": "string",
                        "title": "Term Name"
                    },
                    "courses": {
                        "items": {
                            "$ref": "#/components/schemas/HDCourse"
                        },
                        "type": "array",
                        "title": "Courses",
                        "default": []
                    },
                    "term_gpa": {
                        "type": "number",
                        "title": "Term Gpa",
                        "default": 0.0
                    },
                    "total_credits": {
                        "type": "integer",
                        "title": "Total Credits",
                        "default": 0
                    }
                },
                "type": "object",
                "required": [
                    "term_name"
                ],
                "title": "HDTerm"
            },
            "HDYear": {
                "properties": {
                    "year": {
                        "type": "integer",
                        "title": "Year"
                    },
                    "terms": {
                        "items": {
                            "$ref": "#/components/schemas/HDTerm"
                        },
                        "type": "array",
                        "title": "Terms",
                        "default": []
                    },
                    "year_gpa": {
                        "type": "number",
                        "title": "Year Gpa",
                        "default": 0.0
                    },
                    "total_credits": {
                        "type": "integer",
                        "title": "Total Credits",
                        "default": 0
                    }
                },
                "type": "object",
                "required": [
                    "year"
                ],
                "title": "HDYear"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "IneligibleCourse": {
                "properties": {
                    "course": {
                        "$ref": "#/components/schemas/CourseInfo"
                    },
                    "reasons": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Reasons"
                    }
                },
                "type": "object",
                "required": [
                    "course",
                    "reasons"
                ],
                "title": "IneligibleCourse"
            },
            "RAResponse": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "student_data": {
                        "$ref": "#/components/schemas/StudentData"
                    },
                    "graph_output": {
                        "type": "string",
                        "title": "Graph Output"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "student_data",
                    "graph_output"
                ],
                "title": "RAResponse"
            },
            "Skill": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "score": {
                        "type": "integer",
                        "title": "Score"
                    }
                },
                "type": "object",
                "required": [
                    "name",
                    "score"
                ],
                "title": "Skill"
            },
            "StudentData": {
                "properties": {
                    "eligible_courses": {
                        "additionalProperties": {
                            "items": {
                                "$ref": "#/components/schemas/CourseInfo"
                            },
                            "type": "array"
                        },
                        "type": "object",
                        "title": "Eligible Courses"
                    },
                    "ineligible_courses": {
                        "additionalProperties": {
                            "items": {
                                "$ref": "#/components/schemas/IneligibleCourse"
                            },
                            "type": "array"
                        },
                        "type": "object",
                        "title": "Ineligible Courses"
                    },
                    "category_progress": {
                        "additionalProperties": {
                            "additionalProperties": {
                                "type": "integer"
                            },
                            "type": "object"
                        },
                        "type": "object",
                        "title": "Category Progress"
                    }
                },
                "type": "object",
                "required": [
                    "eligible_courses",
                    "ineligible_courses",
                    "category_progress"
                ],
                "title": "StudentData"
            },
            "StudentLevel": {
                "type": "string",
                "enum": [
                    "Freshman",
                    "Sophomore",
                    "Junior",
                    "Senior"
                ],
                "title": "StudentLevel"
            },
            "StudentResponse": {
                "properties": {
                    "student": {
                        "$ref": "#/components/schemas/CompetenciesStudent"
                    },
                    "comparison_list": {
                        "items": {
                            "$ref": "#/components/schemas/CompetenciesStudent"
                        },
                        "type": "array",
                        "title": "Comparison List"
                    }
                },
                "type": "object",
                "required": [
                    "student",
                    "comparison_list"
                ],
                "title": "StudentResponse"
            },
            "SubCategory": {
                "properties": {
                    "code": {
                        "type": "string",
                        "title": "Code"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "amount": {
                        "type": "number",
                        "title": "Amount"
                    }
                },
                "type": "object",
                "required": [
                    "code",
                    "name",
                    "amount"
                ],
                "title": "SubCategory"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            },
            "XProfile": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "email": {
                        "type": "string",
                        "title": "Email"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    },
                    "major": {
                        "type": "string",
                        "title": "Major",
                        "default": "Computer Science"
                    },
                    "department": {
                        "type": "string",
                        "title": "Department",
                        "default": "Computer Science Department"
                    },
                    "currentSemester": {
                        "type": "integer",
                        "title": "Currentsemester"
                    },
                    "enrolledCourses": {
                        "type": "integer",
                        "title": "Enrolledcourses"
                    },
                    "completedCourses": {
                        "type": "integer",
                        "title": "Completedcourses"
                    },
                    "coursesInProgress": {
                        "type": "integer",
                        "title": "Coursesinprogress"
                    },
                    "skills": {
                        "items": {
                            "$ref": "#/components/schemas/Skill"
                        },
                        "type": "array",
                        "title": "Skills"
                    },
                    "experiences": {
                        "items": {
                            "$ref": "#/components/schemas/Experience"
                        },
                        "type": "array",
                        "title": "Experiences"
                    },
                    "certificates": {
                        "items": {
                            "$ref": "#/components/schemas/Certificate"
                        },
                        "type": "array",
                        "title": "Certificates"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "email",
                    "name",
                    "password",
                    "currentSemester",
                    "enrolledCourses",
                    "completedCourses",
                    "coursesInProgress",
                    "skills",
                    "experiences",
                    "certificates"
                ],
                "title": "XProfile"
            }
        }
    }
}